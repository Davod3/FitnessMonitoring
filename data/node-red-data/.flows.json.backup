    {
        "id": "9bbe960bd0fe1686",
        "type": "function",
        "z": "f720170e2cc34d11",
        "name": "parseDuration",
        "func": "\nvar response = JSON.parse(msg.payload);\n\nvar period = flow.get('timePeriod');\n\nvar runningThreshold = global.get('runningThreshold');\nvar walkingThreshold = global.get('walkingThreshold');\nvar multiplier = 1;\n\nif(runningThreshold === undefined ) {\n    runningThreshold = 10;\n} \n\nif(walkingThreshold === undefined) {\n    walkingThreshold = 30;\n}\n\nif(period === 'week') {\n\n    multiplier = 7;\n\n} else if (period === 'month') {\n\n    multiplier = 30;\n\n}\n\nvar newMsg = {payload : {\n                duration_running : response.data[0][0],\n                duration_walking : response.data[1][0],\n                threshold_running : multiplier * runningThreshold,\n                threshold_walking : multiplier * walkingThreshold\n            }\n        };\n\n\n\nreturn newMsg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
