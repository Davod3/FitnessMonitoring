        "id": "ad6a3cb3572288bf",
        "type": "function",
        "z": "f6f2187d.f17ca8",
        "name": "CreateRequest",
        "func": "var dataPeriod = flow.get('period');\nvar end_date = flow.get('end_date');\nvar start_date = flow.get('start_date');\nvar age = global.get('age');\nvar height = global.get('height');\nvar weight = global.get('weight');\nvar gender = global.get('gender');\nvar walkingThreshold = global.get('walkingThreshold');\nvar runningThreshold = global.get('runningThreshold');\n\nvar isValid = true;\n\nif(dataPeriod === undefined) {\n    dataPeriod = 'day';\n}\n\nif(start_date === undefined || end_date === undefined) {\n    isValid = false;\n    return {payload : 'Error: Please select start and end dates!'};\n}\n\n\nif( new Date(start_date) > new Date(end_date)) {\n    isValid = false;\n    return {payload : 'Error: Start date must be before End date!'};\n}\n\n\nif(age === undefined){\n    age = 21;\n}\n\nif(height === undefined){\n    height = 170;\n}\n\nif(weight === undefined){\n    weight = 50;\n}\n\nif(gender === undefined) {\n    gender = 'male';\n}\n\nif(walkingThreshold === undefined) {\n    walkingThreshold = 30;\n}\n\nif(runningThreshold === undefined) {\n    runningThreshold = 10;\n}\n\nvar newMsg = {\n    payload : {\n        dataPeriod : dataPeriod,\n        start_date : start_date,\n        end_date : end_date,\n        age : age,\n        height : height,\n        weight : weight,\n        gender : gender,\n        walking_threshold : walkingThreshold,\n        running_threshold : runningThreshold\n    }\n};\n\n\nreturn newMsg; //newMsg;",
