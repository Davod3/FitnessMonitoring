    {
        "id": "af6c31310e7e3dd5",
        "type": "function",
        "z": "f720170e2cc34d11",
        "name": "createRequest",
        "func": "var receivedPeriod = msg.payload;\nvar storedPeriod = flow.get('timePeriod');\n\nif(receivedPeriod !== undefined && receivedPeriod !== storedPeriod) {\n    flow.set('timePeriod', receivedPeriod);\n    storedPeriod = receivedPeriod;\n} else if (receivedPeriod === undefined) {\n    flow.set('timePeriod', 'day');\n    storedPeriod = 'day';\n}\n\nvar today = new Date(Date.now());\nvar date = new Date();\ndate.setDate(today.getDate());\ndate.setMonth(today.getMonth());\ndate.setFullYear(today.getFullYear());\ndate.setHours(0);\ndate.setMinutes(0);\ndate.setSeconds(0);\ndate.setMilliseconds(0);\n\nvar age = global.get('age');\nvar height = global.get('height');\nvar weight = global.get('weight');\nvar gender = global.get('gender');\nvar walkingThreshold = global.get('walkingThreshold');\nvar runningThreshold = global.get('runningThreshold');\n\nif(age === undefined){\n    age = 21;\n}\n\nif(height === undefined){\n    height = 170;\n}\n\nif(weight === undefined){\n    weight = 50;\n}\n\nif(gender === undefined) {\n    gender = 'male';\n}\n\nif(walkingThreshold === undefined) {\n    walkingThreshold = 30;\n}\n\nif(runningThreshold === undefined) {\n    runningThreshold = 10;\n}\n\nvar newMsg = {\n    payload: {\n        dataPeriod: storedPeriod,\n        start_date: date.toISOString(),\n        end_date: date.toISOString(),\n        age: age,\n        height: height,\n        weight: weight,\n        gender: gender,\n        walking_threshold: walkingThreshold,\n        running_threshold: runningThreshold\n    }\n}\n\nreturn newMsg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
